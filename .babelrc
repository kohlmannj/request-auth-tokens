{
  "env": {
    "development": {
      "sourceMaps": true,
      "retainLines": true
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  },
  "plugins": [
    "add-module-exports",
    "transform-class-properties",
    "transform-es2015-modules-commonjs"
  ],
  "presets": [
    ["env", {
      "targets": {
        "node": "6.10"
      }
    }],
    "stage-2"
  ]
}
